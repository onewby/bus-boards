			      <xsd:enumeration value="anyone"/>
		    </xsd:restriction>
	  </xsd:simpleType>
   <xsd:simpleType name="EmptyType">
		    <xsd:annotation>
			      <xsd:documentation>A type with no allowed content, used when simply the presence of an element is significant.</xsd:documentation>
		    </xsd:annotation>
