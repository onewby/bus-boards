		    <xsd:annotation>
			      <xsd:documentation>A restriction of W3C XML Schema's string that requires at least one character of text.</xsd:documentation>
		    </xsd:annotation>
		    <xsd:restriction base="xsd:string">
			      <xsd:minLength value="1"/>
		    </xsd:restriction>
	  </xsd:simpleType>
