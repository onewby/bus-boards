					          <xsd:element ref="Extensions" minOccurs="0"/>
				        </xsd:sequence>
		          <xsd:attribute name="version" type="VersionString" default="2.0">
					          <xsd:annotation>
						            <xsd:documentation>Version number of request. Fixed 
</xsd:documentation>
					          </xsd:annotation>
				        </xsd:attribute>
			      </xsd:extension>
		    </xsd:complexContent>
	  </xsd:complexType>
   <xsd:complexType name="ConnectionTimetableSubscriptionStructure">
