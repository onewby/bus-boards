		    <xsd:complexContent>
			      <xsd:extension base="AbstractFeederItemStructure">
				        <xsd:sequence>
					          <xsd:element name="ClearDownRef" type="ClearDownRefStructure" minOccurs="0">
						            <xsd:annotation>
