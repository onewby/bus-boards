				        </xsd:annotation>
			      </xsd:element>
			      <xsd:group ref="InterchangePropertyGroup"/>
			      <xsd:group ref="InterchangeTimesGroup"/>
			      <xsd:element ref="Extensions" minOccurs="0"/>
		    </xsd:sequence>
