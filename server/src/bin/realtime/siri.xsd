		    <xsd:complexContent>
			      <xsd:extension base="AbstractSubscriptionStructure">
				        <xsd:sequence>
					          <xsd:element ref="VehicleMonitoringRequest"/>
					          <xsd:group ref="VehicleMonitoringSubscriptionPolicyGroup"/>
