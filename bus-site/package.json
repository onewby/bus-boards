{
	"name": "bus-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"import:gtfs": "vite-node --options.transformMode.ssr='/.*/' import_gtfs.ts",
		"import:passenger": "vite-node --options.transformMode.ssr='/.*/' import_passenger.ts passenger",
        "import:lothian": "vite-node --options.transformMode.ssr='/.*/' import_lothian.ts lothian",
		"build:realtime": "tsc --esModuleInterop --module esnext --target esnext --moduleResolution node --resolveJsonModule src/realtime/feeder*.ts",
		"service:start": "PORT=3123 node build/index.js",
		"service:realtime": "vite-node --options.transformMode.ssr='/.*/' src/realtime/gtfs_microservice.ts",
		"all": "npm run build:realtime && npm run build && GTFS=microservice concurrently -p '[{time} {name}]' -t 'HH:mm:ss' -c 'cyan,magenta' 'npm:service:realtime' 'npm:service:start'",
		"dev:rt": "GTFS=microservice concurrently -p '[{time} {name}]' -t 'HH:mm:ss' -c 'cyan,magenta' 'npm:service:realtime' 'npm:dev'"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/adapter-node": "^2.0.2",
		"@sveltejs/kit": "^2.1.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/better-sqlite3": "^7.6.3",
		"@types/cli-progress": "^3.11.0",
		"@types/geojson": "^7946.0.10",
		"@types/leaflet": "^1.9.3",
		"@types/luxon": "^3.3.0",
		"@types/node": "^20.10.5",
		"@types/object.groupby": "^1.0.0",
		"@types/proj4": "^2.5.2",
		"@types/unzipper": "^0.10.6",
		"@types/utm": "^1.1.1",
		"@types/ws": "^8.5.10",
		"@vite-pwa/sveltekit": "^0.3.0",
		"autoprefixer": "^10.4.14",
		"node-fetch": "^3.3.2",
		"postcss": "^8.4.33",
		"svelte": "^4.2.8",
		"svelte-check": "^3.5.1",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"ts-proto": "^1.166.2",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vite-node": "^1.1.3"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/core": "^1.5.3",
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@zip.js/zip.js": "^2.7.32",
		"better-sqlite3": "^8.4.0",
		"cli-progress": "^3.12.0",
		"concurrently": "^8.2.2",
		"csv-parse": "^5.4.0",
		"fast-xml-parser": "^4.2.7",
		"geojson": "^0.5.0",
		"hasha": "^5.2.2",
		"json5": "^2.2.3",
		"leaflet": "^1.9.4",
		"luxon": "^3.4.4",
		"object.groupby": "^1.0.1",
		"proj4": "^2.9.0",
		"protobufjs": "^7.2.5",
		"soap": "^1.0.0",
		"svelte-fa": "^4.0.1",
		"svelte-floating-ui": "^1.5.8",
		"svelte-infinite-loading": "^1.3.8",
		"unzipper": "^0.10.14",
		"websocket-promise-lite": "^2.1.1",
		"ws": "^8.16.0"
	}
}
